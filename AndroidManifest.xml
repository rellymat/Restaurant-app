<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bigproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Launch"
            android:label="com.example.bigproject.Launch">
        </activity>
        <activity
            android:name=".Breakfast"
            android:label="com.example.bigproject.Breakfast">
        </activity>
        <activity
            android:name=".Dessert"
            android:label="com.example.bigproject.Dessert">
        </activity>
        <activity android:name=".SList" />
    </application>

</manifest>